<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Board">

    <select id="BoardList"
            resultType="com.project.board.vo.BoardVo">
        SELECT BOARD_NUMBER,
        MENU_ID,
        TITLE,
        WRITER,
        BOARD_LOCAL,
        CONT,
        INDATE

        FROM BOARD

    </select>

    <insert id="InsertBoard" parameterType="com.project.board.vo.BoardVo">
        INSERT INTO BOARD (BOARD_NUMBER,MENU_ID,TITLE,WRITER,CONT)
        VALUES (BOARD_NUMBER.NEXTVAL,'MENU_01',#{title},#{writer},#{cont})


    </insert>



</mapper>